function GetEmotionTypeByMsgStr(msgStr)
	local emotionType = EMOTION_MSGLIST2[msgStr]
	if emotionType ~= nil then
		return emotionType
	else
		return -1
	end
end

function GetEmotionMsg(indexInWnd)
	local emotionType = EMOTION_ORDERLIST[indexInWnd + 1]
	if emotionType ~= nil then
		local emotionMsg = EMOTION_MSGLIST[emotionType]
		if emotionMsg ~= nil then
			return emotionMsg
		else
			return ""
		end
	else
		return ""
	end
end

function GetEmotionType(indexInWnd)
	local emotionType = EMOTION_ORDERLIST[indexInWnd + 1]
	if emotionType ~= nil then
		return emotionType
	else
		return -1
	end
end

function GetEmotionActionByWndIdx(indexInWnd)
	local emotionType = EMOTION_ORDERLIST[indexInWnd + 1]
	if emotionType ~= nil then
		local emotionAction = EMOTION_ACTION[emotionType]
		if emotionAction ~= nil then
			return emotionAction
		else
			return -1
		end
	else
		return -1
	end
end

function GetEmotionAction(emotionType)
	local emotionAction = EMOTION_ACTION[emotionType]
	if emotionAction ~= nil then
		return emotionAction
	else
		return -1
	end
end
