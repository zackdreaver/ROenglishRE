	elseif 3 == Index then
		return worldview_info.SreachMob3_ColorR, worldview_info.SreachMob3_ColorG, worldview_info.SreachMob3_ColorB
	elseif 4 == Index then
		return worldview_info.SreachMob4_ColorR, worldview_info.SreachMob4_ColorG, worldview_info.SreachMob4_ColorB
	elseif 5 == Index then
		return worldview_info.SreachMob5_ColorR, worldview_info.SreachMob5_ColorG, worldview_info.SreachMob5_ColorB
	elseif 10 == Index then
		return worldview_info.SreachNpc_ColorR, worldview_info.SreachNpc_ColorG, worldview_info.SreachNpc_ColorB
	elseif 11 == Index then
		return worldview_info.SreachMob1_ColorR, worldview_info.SreachMob1_ColorG, worldview_info.SreachMob1_ColorB
	elseif 12 == Index then
		return worldview_info.SreachMob2_ColorR, worldview_info.SreachMob2_ColorG, worldview_info.SreachMob2_ColorB
	elseif 13 == Index then
		return worldview_info.SreachMob3_ColorR, worldview_info.SreachMob3_ColorG, worldview_info.SreachMob3_ColorB
	elseif 14 == Index then
		return worldview_info.SreachMob4_ColorR, worldview_info.SreachMob4_ColorG, worldview_info.SreachMob4_ColorB
	elseif 15 == Index then
		return worldview_info.SreachMob5_ColorR, worldview_info.SreachMob5_ColorG, worldview_info.SreachMob5_ColorB
	else
		return Index, Index, Index
	end
end

